{
  "name": "freepbx-cc-dashboard",
  "version": "1.0.0",
  "description": "FreePBX Contact Center Dashboard with Azure Deployment",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server/index.js",
    "connector:dev": "cd local-connector && python main.py",
    "connector:install": "cd local-connector && pip install -r requirements.txt"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.4.4",
    "zustand": "^4.4.7",
    "recharts": "^2.10.3",
    "lucide-react": "^0.300.0",
    "date-fns": "^3.0.6",
    "mysql2": "^3.6.5",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.8",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.4.0",
    "postcss": "^8.4.32",
    "autoprefixer": "^10.4.16"
  }
}